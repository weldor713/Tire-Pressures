<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gallagher.anthony.hour2application">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondaryActivity"
            android:label="@string/title_activity_secondary"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ThirdActivity"
            android:label="@string/title_activity_third"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FourthActivity"
            android:label="@string/title_activity_fourth"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FifthActivity"
            android:label="@string/title_activity_fifth"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SixthActivity"
            android:label="@string/title_activity_sixth"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SeventhActivity"
            android:label="@string/title_activity_seventh"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EighthActivity"
            android:label="@string/title_activity_eighth"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
